{
  "name": "namastereactbackend",
  "version": "1.0.0",
  "description": "This is backend APIs for NamsateReact",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon --watch src --exec ts-node -r tsconfig-paths/register src/index.ts --verbose --legacy-watch",
    "build": "tsc",
    "start": "tsc && nodemon dist/index.js",
    "test": "jest",
    "lint": "eslint . --ext ts",
    "lint:fix": "eslint . --ext ts --fix",
    "typecheck": "tsc --noEmit",
    "prepare": "husky",
    "migrate": "prisma migrate dev",
    "prisma:generate": "prisma generate"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "keywords": [
    "NodeJs",
    "TypeScript"
  ],
  "author": "Nitish Jha",
  "license": "ISC",
  "dependencies": {
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@types/bcrypt": "^6.0.0",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "module-alias": "^2.2.3",
    "pg": "^8.16.3",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.3",
    "@types/pg": "^8.16.0",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "nodemon": "^3.1.11",
    "prettier": "^3.7.4",
    "prisma": "^7.2.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
